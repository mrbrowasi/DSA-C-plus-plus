#include <iostream> 
#include <climits>
#include <cmath>
#include <algorithm>   
#include <vector>
using namespace std;
//Selection sort 
void selectionsort(vector<int> &arr){
    int n = arr.size();
    for(int i = 0; i < n - 1; i++){
        int smallestindex = i;
        for(int j = i+1; j < n; j++){
            if(arr[j] < arr[smallestindex])
            smallestindex = j;
        }
        swap(arr[i], arr[smallestindex]);
    }
    for(int i : arr){
        cout << i << " ";
    }
}
int main(){
    vector<int> arr = {1,2,8,4,9};
    selectionsort(arr);
    return 0;
} 
