#include <iostream> 
#include <climits>
#include <cmath>
#include <algorithm>   
#include <vector>
using namespace std;
//inserted sort 
void insertedsort(vector<int> &arr){
    int n = arr.size();
    for(int i = 1; i < n; i++){
        int current = arr[i];
        int previous = i - 1;
        while(arr[previous] > current && previous >= 0){ // for decending order change > to <
            arr[previous + 1] = arr[previous];
            previous--;
        }
        arr[previous + 1] = current;
    }
    for(int i : arr){
        cout << i << " ";
    }
}
int main(){
    vector<int> arr = {3,5,21,13,4,5,23,1};
    insertedsort(arr);
    return 0;
} 
